rt const Table = () => {\n    const {smallLoading, bigLoading, data, smallVolume, setInfo, addData, error} = useContext(Context)\n    const {sortedData, sortConfig, sortHandler} = useSortableData(data, smallVolume);\n    const {filteredData,  setFilterConfig , notFound} = useFilterableData(sortedData)\n    const {chunkData, chunkNumber, setChunkNumber, totalChunks} = useChunkedData(filteredData, smallVolume)\n    const headerNames = ['id', 'firstName', 'lastName', 'email', 'phone']\n\n\n    const headersMarkup = headerNames.map((header, index) => (\n        <th key={index} scope=\"col\" onClick={() => sortHandler(header)}>\n            {withCapitalLetter(header)} {sortConfig.type === header && (sortConfig.asc ? iconAsc : iconDesc)}\n        </th>\n    ))\n\n\n    if (smallLoading) return <Loader/>\n    if(error) return <Error text={error}/>\n\n    if (chunkData.length) return (\n        <>\n            <TableControls addData={addData} setFilterConfig={setFilterConfig}/>\n            {notFound && <Error text='Not found'/>}\n            <table className=\"table table-hover table-responsive-md mt\" id=\"table\">\n                <thead>\n                <tr>{headersMarkup}</tr>\n                </thead>\n                <tbody>\n                {chunkData.map((p) => {\n                    return (\n                        <tr onClick={() => setInfo(p)} key={p.id + p.firstName}>\n                            <th scope=\"row\">{p.id}</th>\n                            <td>{withCapitalLetter(p.firstName)}</td>\n                            <td>{withCapitalLetter(p.lastName)}</td>\n                            <td>{p.email}</td>\n                            <td>{p.phone}</td>\n                        </tr>)\n                })}\n                </tbody>\n            </table>\n            {bigLoading && <Loader/>}\n            {totalChunks > 1 && <Pagination chunkNumber={chunkNumber} setChunkNumber={setChunkNumber} totalChunks={totalChunks}/>}\n        </>\n    )\n    return <p className=\"text-muted text-center\">Determine the amount of data to load {topIcon}</p>\n}\n","/Users/julia/Desktop/datafinder/src/context/types.js",[],"/Users/julia/Desktop/datafinder/src/context/reduser.js",[],"/Users/julia/Desktop/datafinder/src/context/context.js",[],"/Users/julia/Desktop/datafinder/src/icons.js",[],"/Users/julia/Desktop/datafinder/src/utils.js",[],"/Users/julia/Desktop/datafinder/src/hooks/sorting.hook.js",[],"/Users/julia/Desktop/datafinder/src/hooks/filtration.hook.js",[],"/Users/julia/Desktop/datafinder/src/hooks/chunking.hook.js",[],"/Users/julia/Desktop/datafinder/src/components/Loader.jsx",[],"/Users/julia/Desktop/datafinder/src/components/Pagination.jsx",[],"/Users/julia/Desktop/datafinder/src/components/TableControls.jsx",[],"/Users/julia/Desktop/datafinder/src/components/Error.jsx",[],"/Users/julia/Desktop/datafinder/src/hooks/formProcessing.hook.js",[],"/Users/julia/Desktop/datafinder/src/components/FormControls.jsx",[],"/Users/julia/Desktop/datafinder/src/components/FilterControls.jsx",[],"/Users/julia/Desktop/datafinder/src/components/Form.jsx",[],"/Users/julia/Desktop/datafinder/src/components/Input.jsx",[],"/Users/julia/Desktop/test/src/index.js",[],"/Users/julia/Desktop/test/src/reportWebVitals.js",[],"/Users/julia/Desktop/test/src/App.jsx",[],"/Users/julia/Desktop/test/src/context/state.js",[],"/Users/julia/Desktop/test/src/components/Header.jsx",[],"/Users/julia/Desktop/test/src/components/Info.jsx",[],"/Users/julia/Desktop/test/src/components/Table.jsx",[],"/Users/julia/Desktop/test/src/components/ChoiceVolume.jsx",[],"/Users/julia/Desktop/test/src/context/reduser.js",[],"/Users/julia/Desktop/test/src/context/types.js",[],"/Users/julia/Desktop/test/src/context/context.js",[],"/Users/julia/Desktop/test/src/utils.js",[],"/Users/julia/Desktop/test/src/icons.js",[],"/Users/julia/Desktop/test/src/hooks/sorting.hook.js",[],"/Users/julia/Desktop/test/src/hooks/filtration.hook.js",[],["209","210"],"/Users/julia/Desktop/test/src/hooks